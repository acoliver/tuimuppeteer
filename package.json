{
  "name": "tuimuppeteer",
  "version": "1.0.0",
  "description": "Deterministic TUI harness",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "tuimuppeteer": "src/cli/main.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "bun": "./src/index.ts"
    },
    "./cli": {
      "import": "./src/cli/main.ts",
      "bun": "./src/cli/main.ts"
    }
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "lint": "bun x eslint \"**/*.{ts,tsx}\"",
    "test": "bun x vitest run",
    "typecheck": "bun x tsc --noEmit",
    "check": "bun x eslint \"**/*.{ts,tsx}\" && bun x tsc --noEmit && bun x vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20",
    "bun": ">=1.2.0"
  },
  "dependencies": {
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.5.1",
    "eslint": "^9.39.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
